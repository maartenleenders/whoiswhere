{
   "type": "postgres",
   "database": "whoiswhere",
   "host": "whoiswhere-postgresql",
   "port": 5432,
   "username": "postgres",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/**/**.entity{.ts,.js}"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}
